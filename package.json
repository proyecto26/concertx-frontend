{
  "name": "concertx-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:styles -- --env production",
    "build:demo": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:styles -- -w",
    "dev:demo": "remix dev",
    "start": "run-p start:*",
    "start:css": "npm run build:css",
    "start:demo": "remix-serve build",
    "generate:styles": "postcss styles/**/*.css --base styles --dir app/styles"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@remix-run/node": "^1.8.1",
    "@remix-run/react": "^1.8.1",
    "@remix-run/serve": "^1.8.1",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.1",
    "@remix-run/eslint-config": "^1.8.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.0.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
